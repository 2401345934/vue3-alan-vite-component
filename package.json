{
  "name": "@xiaomh/vue3-alan-vite-component",
  "private": false,
  "version": "1.1.5",
  "keywords": [
    "xmh-cli-test"
  ],
  "scripts": {
    "start": "vite --force --host",
    "release": "release-it",
    "build": "vite build",
    "commit": "npm run changelog && git add . && git cz ",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "version": "zx bump.mjs",
    "prepublish": "npm run version && npm run build",
    "publish": "npm run commit && npm publish",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "files": [
    "lib"
  ],
  "main": "lib/vue3-alan-vite-component.umd.js",
  "module": "lib/vue3-alan-vite-component.es.js",
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "release-it": "^15.5.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "vite": "^2.9.9",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "zx": "^7.1.1"
  }
}