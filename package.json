{
  "name": "vue3-alan-vite-component",
  "private": false,
  "version": "0.1.3",
  "keywords": [
    "xmh-cli-test"
  ],
  "scripts": {
    "dev": "vite --force --host",
    "build": "vite build",
    "commit": "npm run changelog && git add . && git cz ",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "publish": "npm publish",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "files": [
    "lib"
  ],
  "main": "lib/vue3-alan-vite-component.umd.js",
  "module": "lib/vue3-alan-vite-component.es.js",
  "exports": {
    "./lib/style.css": "./lib/style.css",
    ".": {
      "import": "./lib/vue3-alan-vite-component.es.js",
      "require": "./lib/vue3-alan-vite-component.umd.js"
    }
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "vite": "^2.9.9"
  }
}
